import random
import pygame
from pygame.locals import *

pygame.init()

display_width = 800
display_height = 600

gamesurface = pygame.display.set_mode((display_width_display_height))
pygame.display.set_caption("Whack a Mole")

gameExit = False

while not gameExit:
    for event in pygame.event.get():
        print(event)

    pygame.quit()
    quit()